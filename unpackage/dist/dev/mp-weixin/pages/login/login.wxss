.uni-input-wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  /* padding: 8px 13px; */
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  background-color: #FFFFFF;
}
.image-wrapper {
  height: 240rpx;
}
.image {
  height: 200rpx;
  width: 200rpx;
}
.uni-icon {
  font-family: uniicons;
  font-size: 24px;
  font-weight: normal;
  font-style: normal;
  width: 24px;
  height: 24px;
  margin-top: 15rpx;
  margin-right: 20rpx;
  line-height: 24px;
  color: #999999;
}
.uni-eye-active {
  color: #007AFF;
}
.foot {
  position: fixed;
  left: 0;
  bottom: 30rpx;
  width: 750rpx;
  text-align: center;
}
.foot view:nth-child(1) {
  color: #444;
}
.canvas-img-code {
  display: inline-block;
}

